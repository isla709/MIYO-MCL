        <Canvas>
            <Rectangle Height="90" Canvas.Top="60" Width="320" HorizontalAlignment="Center" VerticalAlignment="Top" RadiusX="10" RadiusY="10" Fill="#CC000000" Canvas.Left="-10"/>
